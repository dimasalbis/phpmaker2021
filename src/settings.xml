        <setting name="Show current filters" id="ShowCurrentFilter" type="Boolean" value="0" />
        <setting name="Show blank series for chart" id="ShowBlankSeriesForChart" type="Boolean" value="0" />
        <setting name="Show zero values in stacked chart" id="ShowZeroInStackChart" type="Boolean" value="0" />
        <setting name="Use popup panel for drill down" id="UseDrillDownPanel" type="Boolean" value="1" />
        <setting name="Show drill down filters" id="ShowDrillDownFilter" type="Boolean" value="1" />
        <setting name="Chart column CSS class (left or right)" id="ChartLeftOrRightColumnClass" options="col-sm-3,col-sm-4,col-sm-6" value="col-sm-6" />
        <setting name="Chart column CSS class (left and right)" id="ChartLeftAndRightColumnClass" options="col-sm-3,col-sm-4" value="col-sm-4" />
        <setting name="Chart color palette" id="ChartColorPalette" value="#5899DA,#E8743B,#19A979,#ED4A7B,#945ECF,#13A4B4,#525DF4,#BF399E,#6C8893,#EE6868,#2F6497" />
