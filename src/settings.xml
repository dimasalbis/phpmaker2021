        <setting name="RewriteBase directive" id="RewriteBase" value="" /><!-- v2021 -->
        <!-- API -->
        <setting name="API JWT signing secret key" id="ApiJwtSecretKey" value="" /><!-- v2019 -->
        <setting name="API JWT signing algorithm" id="ApiJwtAlgorithm" value="HS512" /><!-- v2019 -->
        <setting name="API JWT authorization header" id="ApiJwtAuthHeader" value="X-Authorization" /><!-- v2019 -->
        <setting name="API access time after login (seconds)" id="ApiAccessTimeAfterLogin" type="Integer" value="0" /><!-- v2019 -->
        <setting name="API expire time after login (seconds)" id="ApiExpireTimeAfterLogin" type="Integer" value="600" /><!-- v2019 -->
        <setting name="API custom headers" id="ApiCustomHeaders" type="Boolean" value="0" /><!-- v2021 -->
        <setting name="API Access-Control-Allow-Origin" id="AccessControlAllowOrigin" value="*" /><!-- v2021 -->
        <setting name="API Access-Control-Allow-Headers" id="AccessControlAllowHeaders" value="*" /><!-- v2021 -->
        <setting name="Use Swagger UI for API" id="UseSwaggerUI" type="Boolean" value="1" /><!-- v2021 -->
        <setting name="Session-less API actions" id="SessionlessApiActions" value="file" /><!-- v2021 -->
